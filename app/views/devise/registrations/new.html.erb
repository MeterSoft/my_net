<p class="top_login">
  Registration
</p>
<div class="mlogin">
    <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
      <div><%= f.email_field :email, :placeholder => 'Email' %></div><br>

      <div><%= f.password_field :password, :placeholder => 'Password' %></div><br>

      <div><%= f.password_field :password_confirmation, :placeholder => 'Password' %></div>

      <div>
        <%= link_to "Sign in", new_session_path(resource_name), :class => 'style_new_sessions' %>
        <%= f.submit "Sign up" %>
      </div>
    <% end %>
</div>
<p class="futer-login">
  <%= link_to "Sign in", new_session_path(resource_name)%>
</p>


